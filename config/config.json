{
    "hosts": {
        "main": {
            "protocol": "http",
            "sub_domain": null,
            "domain": "MAIN_HOST",
            "env": true
        },
        "dev": {
            "protocol": "http",
            "sub_domain": null,
            "domain": "REMOTE_IP",
            "env": true
        },
        "admin": {
            "protocol": "https",
            "sub_domain": "my-akinyele-admin",
            "domain": "herokuapp.com"
        },
        "admin-test": {
            "protocol": "http",
            "sub_domain": null,
            "domain": "localhost:4000"
        }
    },
    "videojs": {
        "version": "5.16.0"
    },
    "api": {
        "host": "https://anilist.co/api",
        "env": {
            "client_id": "TANOSHIMU_CLIENT_ID",
            "secret_id": "TANOSHIMU_CLIENT_SECRET"
        },
        "uris": {
            "req_token": "Post|/auth/access_token"
        }
